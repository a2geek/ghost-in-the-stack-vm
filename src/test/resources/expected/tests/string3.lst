MAIN:
    DIM _ONERR_TARGET AS ADDRESS = null
    DIM _ONERR_FP AS ADDRESS = null
    DIM _ONERR_SP AS ADDRESS = null
    DIM A$ AS STRING = [""]
    DIM B$ AS STRING = [""]
    DIM C$ AS STRING = [""]
    DIM D$ AS STRING = [""]
    DIM EXPECTED$ AS STRING = [""]
    DIM _TEMP7 AS STRING = [""]
    DIM _TEMP15 AS INTEGER = null
    DIM _TEMP16 AS STRING = [""]
    DIM _TEMP17 AS INTEGER = null
    DIM _TEMP18 AS STRING = [""]
    DIM _TEMP19 AS INTEGER = null
    DIM _TEMP20 AS STRING = [""]
    DIM _TEMP21 AS INTEGER = null
   0:     EXPECTED$ = "Hello, World!"
   1:     A$ = "Hello"
   2:     B$ = ", "
   3:     C$ = "World!"
   4:     CALL RUNTIME.PRINT_STRING(A$)
   5:     CALL RUNTIME.PRINT_STRING(B$)
   6:     CALL RUNTIME.PRINT_STRING(C$)
   7:     CALL RUNTIME.PRINT_NEWLINE()
   8:     _TEMP15 = ((STRINGS.LEN(A$) + STRINGS.LEN(B$)) + 2)
   9:     _TEMP7 = ALLOC(_TEMP15)
  10:     *(_TEMP7) = CByte((STRINGS.LEN(A$) + STRINGS.LEN(B$)))
  11:     CALL STRINGS.STRCAT(_TEMP7, A$)
  12:     CALL STRINGS.STRCAT(_TEMP7, B$)
  13:     _TEMP17 = ((STRINGS.LEN(_TEMP7) + STRINGS.LEN(C$)) + 2)
  14:     _TEMP16 = ALLOC(_TEMP17)
  15:     *(_TEMP16) = CByte((STRINGS.LEN(_TEMP7) + STRINGS.LEN(C$)))
  16:     CALL STRINGS.STRCAT(_TEMP16, _TEMP7)
  17:     CALL STRINGS.STRCAT(_TEMP16, C$)
  18:     D$ = _TEMP16
  19:     CALL RUNTIME.PRINT_STRING(D$)
  20:     CALL RUNTIME.PRINT_NEWLINE()
  21:     CALL TEXT.INVERSE()
  22:     _TEMP19 = ((STRINGS.LEN(A$) + STRINGS.LEN(B$)) + 2)
  23:     _TEMP18 = ALLOC(_TEMP19)
  24:     *(_TEMP18) = CByte((STRINGS.LEN(A$) + STRINGS.LEN(B$)))
  25:     CALL STRINGS.STRCAT(_TEMP18, A$)
  26:     CALL STRINGS.STRCAT(_TEMP18, B$)
  27:     _TEMP21 = ((STRINGS.LEN(_TEMP18) + STRINGS.LEN(C$)) + 2)
  28:     _TEMP20 = ALLOC(_TEMP21)
  29:     *(_TEMP20) = CByte((STRINGS.LEN(_TEMP18) + STRINGS.LEN(C$)))
  30:     CALL STRINGS.STRCAT(_TEMP20, _TEMP18)
  31:     CALL STRINGS.STRCAT(_TEMP20, C$)
  32:     IF ((STRINGS.STRCMP(D$, EXPECTED$) = 0) and (STRINGS.STRCMP(_TEMP20, EXPECTED$) = 0)) THEN
   0:         CALL RUNTIME.PRINT_STRING("SUCCESS")
   1:         CALL RUNTIME.PRINT_NEWLINE()
          ELSE
   0:         CALL RUNTIME.PRINT_STRING("FAIL")
   1:         CALL RUNTIME.PRINT_NEWLINE()
          END IF
  33:     CALL TEXT.NORMAL()
  34:     END

