MAIN:
    DIM _ONERR_TARGET AS ADDRESS = null
    DIM _ONERR_FP AS ADDRESS = null
    DIM _ONERR_SP AS ADDRESS = null
   0:     CALL DISPLAY(1, 2, 3)
   1:     CALL DISPLAY(3, 2, 1)
   2:     END

SUB DISPLAY(BYVAL C AS INTEGER, BYVAL B AS INTEGER, BYVAL A AS INTEGER)
   0:     CALL RUNTIME.PRINT_STRING("A=")
   1:     CALL RUNTIME.PRINT_INTEGER(A)
   2:     CALL RUNTIME.PRINT_COMMA()
   3:     CALL RUNTIME.PRINT_STRING("B=")
   4:     CALL RUNTIME.PRINT_INTEGER(B)
   5:     CALL RUNTIME.PRINT_COMMA()
   6:     CALL RUNTIME.PRINT_STRING("C=")
   7:     CALL RUNTIME.PRINT_INTEGER(C)
   8:     CALL RUNTIME.PRINT_NEWLINE()
END SUB

