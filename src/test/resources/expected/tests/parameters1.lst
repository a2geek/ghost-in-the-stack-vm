MAIN:
    DIM _ONERR_TARGET AS ADDRESS = null
    DIM _ONERR_FP AS ADDRESS = null
    DIM _ONERR_SP AS ADDRESS = null
   0:     CALL RUNTIME.PRINT_STRING("TESTING DEFAULT PARAMETER VALUE (-1)")
   1:     CALL RUNTIME.PRINT_NEWLINE()
   2:     CALL TESTING(1, (- 1))
   3:     CALL TESTING(2, 3)
   4:     CALL TESTING(4, (- 1))
   5:     CALL TESTING(5, 6)
   6:     CALL TESTING(7, (- 1))
   7:     CALL RUNTIME.PRINT_STRING("END")
   8:     CALL RUNTIME.PRINT_NEWLINE()
   9:     END

SUB TESTING(BYVAL B AS INTEGER = (- 1), BYVAL A AS INTEGER)
   0:     CALL RUNTIME.PRINT_STRING("TESTING: A=")
   1:     CALL RUNTIME.PRINT_INTEGER(A)
   2:     CALL RUNTIME.PRINT_STRING(", B=")
   3:     CALL RUNTIME.PRINT_INTEGER(B)
   4:     CALL RUNTIME.PRINT_NEWLINE()
END SUB

