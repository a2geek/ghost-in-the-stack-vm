MAIN:
    DIM _ONERR_TARGET AS ADDRESS = null
    DIM _ONERR_FP AS ADDRESS = null
    DIM _ONERR_SP AS ADDRESS = null
    DIM CH AS INTEGER = null
   0:     CH = 177
   1:     CALL RUNTIME.PRINT_STRING("CH=")
   2:     CALL RUNTIME.PRINT_INTEGER(CH)
   3:     CALL RUNTIME.PRINT_STRING("  0=")
   4:     CALL RUNTIME.PRINT_INTEGER(STRINGS.ASC("0"))
   5:     CALL RUNTIME.PRINT_STRING("  9=")
   6:     CALL RUNTIME.PRINT_INTEGER(STRINGS.ASC("9"))
   7:     CALL RUNTIME.PRINT_NEWLINE()
   8:     IF ((CH >= STRINGS.ASC("0")) and (CH <= STRINGS.ASC("9"))) THEN
   0:         CALL RUNTIME.PRINT_STRING(">= 0 AND <= 9")
   1:         CALL RUNTIME.PRINT_NEWLINE()
          END IF
   9:     IF (CH >= STRINGS.ASC("0")) THEN
   0:         CALL RUNTIME.PRINT_STRING(">= 0")
   1:         CALL RUNTIME.PRINT_NEWLINE()
          END IF
  10:     IF (CH <= STRINGS.ASC("9")) THEN
   0:         CALL RUNTIME.PRINT_STRING("<= 9")
   1:         CALL RUNTIME.PRINT_NEWLINE()
          END IF
  11:     END

