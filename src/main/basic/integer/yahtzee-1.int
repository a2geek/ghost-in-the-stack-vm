3030 IF S(12)=0 THEN 3040:T=12:RETURN :REM  SCORE YAHTZEE
3040 IF S(11)=0 THEN 3050:T=11:RETURN :REM  SCORE LG.STRT
3050 IF S(9)=0 THEN 3060:T=9:RETURN :REM  SCORE SM.STRT
