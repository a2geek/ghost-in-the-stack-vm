3440 GOTO 3560
3450 J=5:K=0
3460 J=J-1:IF D1(J)#D1(J+1) THEN 3490
3470 T=D1(J)
3480 K=1:IF SC(PP+D1(J))=-1 THEN 3360
3490 IF J>1 THEN 3460
